{
  "name": "artiqui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .js,.vue",
    "prepare": "husky install",
    "watch": "chokidar \"src/**/*.{vue,js}\" \"assets/sass/**/*.{scss}\" -c \"npm run build\""
  },
  "dependencies": {
    "bootstrap": "^4.6.2",
    "core-js": "^3.8.3",
    "flatpickr": "^4.6.13",
    "vue": "^2.6.14"
  },
  "lint-staged": {
  "**/*": "prettier --write --ignore-unknown"
},
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "^5.0.8",
    "chokidar-cli": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^9.1.7",
    "vite": "^4.5.14",
    "vite-plugin-live-reload": "^3.0.5",
    "vite-plugin-vue2": "^2.0.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
